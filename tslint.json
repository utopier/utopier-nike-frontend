{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "rules": {
      "max-classes-per-file": false,
      "jsx-no-lambda": false,
      "no-console": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "only-arrow-functions": false,
      "curly": false,
      "member-access":false
    },
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js",
        "src/countries.ts"
      ]
    }
  }